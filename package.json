{
	"name": "prep-on-the-go",
	"version": "0.0.1",
	"description": "An app for",
	"author": "Temitope Bamidele",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "node dist/main",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "bash ./test/test.sh",
		"compodoc": "npx compodoc -p tsconfig.json -s",
		"migrate": "node dist/migrations/usersAddRefreshToken"
	},
	"dependencies": {
		"@nestjs/bull": "^0.1.2",
		"@nestjs/common": "^7.0.0",
		"@nestjs/core": "^7.0.0",
		"@nestjs/mongoose": "^7.0.2",
		"@nestjs/passport": "^7.1.0",
		"@nestjs/platform-express": "^7.0.0",
		"@nestjs/swagger": "^4.5.12",
		"@sendgrid/mail": "^7.2.1",
		"@timberio/node": "^0.35.0",
		"@types/mongoose-paginate": "^5.0.8",
		"aws-sdk": "^2.718.0",
		"bcrypt": "^5.0.0",
		"body-parser": "^1.19.0",
		"bull": "^3.16.0",
		"cache-manager": "^3.3.0",
		"cache-manager-redis-store": "^2.0.0",
		"class-transformer": "^0.2.3",
		"class-validator": "^0.12.2",
		"dotenv": "^8.2.0",
		"express-rate-limit": "^5.1.3",
		"handlebars": "^4.7.6",
		"handlebars-intl": "^1.1.2",
		"helmet": "^3.23.3",
		"jsonwebtoken": "^8.5.1",
		"moment": "^2.27.0",
		"mongodb": "^3.6.0",
		"mongoose": "^5.9.25",
		"mongoose-paginate": "^5.0.3",
		"passport": "^0.4.1",
		"passport-jwt": "^4.0.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^6.5.4",
		"sendgrid": "^5.2.3",
		"short-unique-id": "^3.0.4",
		"shortid": "^2.2.15",
		"swagger-ui-express": "^4.1.4",
		"uuid": "^8.2.0"
	},
	"devDependencies": {
		"@compodoc/compodoc": "^1.1.11",
		"@nestjs/cli": "^7.0.0",
		"@nestjs/schematics": "^7.0.0",
		"@nestjs/testing": "^7.0.0",
		"@timberio/types": "^0.35.0",
		"@types/aws-sdk": "^2.7.0",
		"@types/bull": "^3.14.0",
		"@types/express": "^4.17.3",
		"@types/jest": "25.2.3",
		"@types/mongoose": "^5.7.32",
		"@types/multer": "^1.4.3",
		"@types/node": "^13.13.14",
		"@types/passport": "^1.0.4",
		"@types/passport-jwt": "^3.0.3",
		"@types/shortid": "0.0.29",
		"@types/supertest": "^2.0.8",
		"@typescript-eslint/eslint-plugin": "3.0.2",
		"@typescript-eslint/parser": "3.0.2",
		"eslint": "7.1.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-import": "^2.20.1",
		"jest": "26.0.1",
		"prettier": "^1.19.1",
		"supertest": "^4.0.2",
		"ts-jest": "26.1.0",
		"ts-loader": "^6.2.1",
		"ts-node": "^8.6.2",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^3.7.4"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": ".",
		"testEnvironment": "node",
		"testRegex": ".e2e-spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coveragePathIgnorePatterns": [
			"/node_modules/"
		],
		"verbose": true
	}
}
